@prefix : <http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/> .

<http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology> rdf:type owl:Ontology ;
                                                                     rdfs:comment "An Ontology for flight satisfaction surveys. Made by David Cucalon"@en ;
                                                                     rdfs:label "esvOntology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasADelayOf
:hasADelayOf rdf:type owl:ObjectProperty ;
             rdfs:domain :Trip ;
             rdfs:range :Delay ;
             rdfs:comment "Has a delay of"@en ;
             rdfs:label "hasADelayOf"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasAJobIn
:hasAJobIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Pilot ,
                       :Stewardess ;
           rdfs:range :Airline ;
           rdfs:comment "has a job in"@en ;
           rdfs:label "hasAJobIn"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasANumberOfSeats
:hasANumberOfSeats rdf:type owl:ObjectProperty ;
                   rdfs:domain :Aircraft ;
                   rdfs:range :Seat ;
                   rdfs:comment "has a number of seats"@en ;
                   rdfs:label "hasANumberOfSeats"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasAirplainsOf
:hasAirplainsOf rdf:type owl:ObjectProperty ;
                rdfs:domain :Airline ;
                rdfs:range :Aircraft ;
                rdfs:comment "has airplains of"@en ;
                rdfs:label "hasAirplainsOf"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasAnETA
:hasAnETA rdf:type owl:ObjectProperty ;
          rdfs:domain :Trip ;
          rdfs:range :ArrivalAt ;
          rdfs:comment "has an estimated time of arrival (ETA)"@en ;
          rdfs:label "hasAnETA"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasAnETD
:hasAnETD rdf:type owl:ObjectProperty ;
          rdfs:domain :Trip ;
          rdfs:range :TakeOffAt ;
          rdfs:comment "has an estimated time of departure (ETD)"@en ;
          rdfs:label "hasAnETD"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasBoughtA
:hasBoughtA rdf:type owl:ObjectProperty ;
            rdfs:domain :Passenger ;
            rdfs:range :Ticket ;
            rdfs:comment "bought a ticket"@en ;
            rdfs:label "hasBoughtA"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasWritten
:hasWritten rdf:type owl:ObjectProperty ;
            rdfs:domain :Passenger ;
            rdfs:range :Review ;
            rdfs:comment "has written"@en ;
            rdfs:label "hasWritten"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isAssociatedWith
:isAssociatedWith rdf:type owl:ObjectProperty ;
                  rdfs:domain :Luggage ;
                  rdfs:range :Ticket ;
                  rdfs:comment "is associated with"@en ;
                  rdfs:label "isAssociatedWith"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isBuildBy
:isBuildBy rdf:type owl:ObjectProperty ;
           rdfs:domain :Aircraft ;
           rdfs:range :AircraftManufacturer ;
           rdfs:comment "is build by"@en ;
           rdfs:label "isBuildBy"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isComingFrom
:isComingFrom rdf:type owl:ObjectProperty ;
              rdfs:domain :Trip ;
              rdfs:range :Airport ;
              rdfs:comment "is coming from"@en ;
              rdfs:label "isComingFrom"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isFor
:isFor rdf:type owl:ObjectProperty ;
       rdfs:domain :Ticket ;
       rdfs:range :Trip ;
       rdfs:comment "is for"@en ;
       rdfs:label "isFor"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isFrom
:isFrom rdf:type owl:ObjectProperty ;
        rdfs:domain :City ;
        rdfs:range :Country ;
        rdfs:comment "is from"@en ;
        rdfs:label "isFrom"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isGoingTo
:isGoingTo rdf:type owl:ObjectProperty ;
           rdfs:domain :Trip ;
           rdfs:range :Airport ;
           rdfs:comment "is going to"@en ;
           rdfs:label "isGoingTo"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isLocatedIn
:isLocatedIn rdf:type owl:ObjectProperty ;
             rdfs:domain :Airport ;
             rdfs:range :City ;
             rdfs:comment "is located in"@en ;
             rdfs:label "isLocatedIn"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isRelatedWith
:isRelatedWith rdf:type owl:ObjectProperty ;
               rdfs:domain :Review ;
               rdfs:range :Ticket ;
               rdfs:comment "is related with"@en ;
               rdfs:label "isRelatedWith"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isResponsibleOf
:isResponsibleOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :Pilot ;
                 rdfs:range :Aircraft ;
                 rdfs:comment "is responsible of"@en ;
                 rdfs:label "isResponsibleOf"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isTakeIn
:isTakeIn rdf:type owl:ObjectProperty ;
          rdfs:domain :Trip ;
          rdfs:range :Aircraft ;
          rdfs:comment "is take in"@en ;
          rdfs:label "isTakeIn"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isWorkingIn
:isWorkingIn rdf:type owl:ObjectProperty ;
             rdfs:domain :Stewardess ;
             rdfs:range :Aircraft ;
             rdfs:comment "is working in"@en ;
             rdfs:label "isWorkingIn"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/DatePublished
:DatePublished rdf:type owl:DatatypeProperty ;
               rdfs:domain :Review ;
               rdfs:range xsd:dateTimeStamp ;
               rdfs:comment "Date when the review was published"@en ;
               rdfs:label "DatePublished"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasAge
:hasAge rdf:type owl:DatatypeProperty ;
        rdfs:domain :Person ;
        rdfs:range [ rdf:type rdfs:Datatype ;
                     owl:onDatatype xsd:integer ;
                     owl:withRestrictions ( [ xsd:minExclusive 0
                                            ]
                                          )
                   ] ;
        rdfs:comment "Person age"@en ;
        rdfs:label "hasAge"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasAircraftName
:hasAircraftName rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Aircraft ;
                 rdfs:range xsd:string ;
                 rdfs:comment "aircraft name"@en ;
                 rdfs:label "hasAircraftName"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasAirlineName
:hasAirlineName rdf:type owl:DatatypeProperty ;
                rdfs:domain :Airline ;
                rdfs:range xsd:string ;
                rdfs:comment "airline name"@en ;
                rdfs:label "hasAirlineName"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasAirportName
:hasAirportName rdf:type owl:DatatypeProperty ;
                rdfs:domain :Airport ;
                rdfs:range xsd:string ;
                rdfs:comment "airport name"@en ;
                rdfs:label "hasAirportName"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasArrivalTimezone
:hasArrivalTimezone rdf:type owl:DatatypeProperty ;
                    rdfs:domain :ArrivalAt ;
                    rdfs:range xsd:string ;
                    rdfs:comment "arrival time zone"@en ;
                    rdfs:label "hasArrivalTimezone"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasCityName
:hasCityName rdf:type owl:DatatypeProperty ;
             rdfs:domain :City ;
             rdfs:range xsd:string ;
             rdfs:comment "city name"@en ;
             rdfs:label "hasCityName"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasComment
:hasComment rdf:type owl:DatatypeProperty ;
            rdfs:domain :Review ;
            rdfs:range xsd:string ;
            rdfs:comment "The rewiew body or comment"@en ;
            rdfs:label "hasComment"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasCountryName
:hasCountryName rdf:type owl:DatatypeProperty ;
                rdfs:domain :Country ;
                rdfs:range xsd:string ;
                rdfs:comment "country name"@en ;
                rdfs:label "hasCountryName"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasDelayReason
:hasDelayReason rdf:type owl:DatatypeProperty ;
                rdfs:domain :Delay ;
                rdfs:range xsd:string ;
                rdfs:comment "The reason of the delay"@en ;
                rdfs:label "hasDelayReason"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasFlightDateTime
:hasFlightDateTime rdf:type owl:DatatypeProperty ;
                   rdfs:domain :FlightDateTime ;
                   rdfs:range xsd:dateTimeStamp ;
                   rdfs:comment "The flight date and time"@en ;
                   rdfs:label "hasFlightDateTime"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasFlightHours
:hasFlightHours rdf:type owl:DatatypeProperty ;
                rdfs:domain :CrewMember ;
                rdfs:range [ rdf:type rdfs:Datatype ;
                             owl:onDatatype xsd:integer ;
                             owl:withRestrictions ( [ xsd:minExclusive 0
                                                    ]
                                                  )
                           ] ;
                rdfs:comment "The number of flight hours"@en ;
                rdfs:label "hasFlightHours"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasIATACode
:hasIATACode rdf:type owl:DatatypeProperty ;
             rdfs:domain :Airline ;
             rdfs:range xsd:string ;
             rdfs:comment "The IATA code of the company"@en ;
             rdfs:label "hasIATACode"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasManufacturerName
:hasManufacturerName rdf:type owl:DatatypeProperty ;
                     rdfs:domain :AircraftManufacturer ;
                     rdfs:range xsd:string ;
                     rdfs:comment "manufacturer name"@en ;
                     rdfs:label "hasManufacturerName"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasNumberOfSeats
:hasNumberOfSeats rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Aircraft ;
                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:onDatatype xsd:integer ;
                               owl:withRestrictions ( [ xsd:minExclusive 0
                                                      ]
                                                    )
                             ] ;
                  rdfs:comment "the number of the seats"@en ;
                  rdfs:label "hasNumberOfSeats"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasPersonName
:hasPersonName rdf:type owl:DatatypeProperty ;
               rdfs:domain :Person ;
               rdfs:range xsd:string ;
               rdfs:comment "Person name"@en ;
               rdfs:label "hasPersonName"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasPersonSurname
:hasPersonSurname rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Person ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Person surname"@en ;
                  rdfs:label "hasPersonSurname"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasPilotRange
:hasPilotRange rdf:type owl:DatatypeProperty ;
               rdfs:domain :Pilot ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:oneOf [ rdf:type rdf:List ;
                                        rdf:first "Cadet" ;
                                        rdf:rest [ rdf:type rdf:List ;
                                                   rdf:first "Captain" ;
                                                   rdf:rest [ rdf:type rdf:List ;
                                                              rdf:first "Officer" ;
                                                              rdf:rest rdf:nil
                                                            ]
                                                 ]
                                      ]
                          ] ;
               rdfs:comment "The pilot range"@en ;
               rdfs:label "hasPilotRange"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasRating
:hasRating rdf:type owl:DatatypeProperty ;
           rdfs:domain :Review ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:onDatatype xsd:integer ;
                        owl:withRestrictions ( [ xsd:minExclusive "0"^^xsd:int
                                               ]
                                               [ xsd:maxInclusive 10
                                               ]
                                             )
                      ] ;
           rdfs:comment "The rating of the review"@en ;
           rdfs:label "hasRating"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasSeatCode
:hasSeatCode rdf:type owl:DatatypeProperty ;
             rdfs:domain :Seat ;
             rdfs:range xsd:string ;
             rdfs:comment "The code of the seat"@en ;
             rdfs:label "hasSeatCode"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasTakeOffTimeZone
:hasTakeOffTimeZone rdf:type owl:DatatypeProperty ;
                    rdfs:domain :TakeOffAt ;
                    rdfs:range xsd:string ;
                    rdfs:comment "The date and time when the flight departure"@en ;
                    rdfs:label "hasTakeOffTimeZone"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasTicketFee
:hasTicketFee rdf:type owl:DatatypeProperty ;
              rdfs:domain :Ticket ;
              rdfs:range [ rdf:type rdfs:Datatype ;
                           owl:onDatatype xsd:float ;
                           owl:withRestrictions ( [ xsd:minExclusive "0.0"^^xsd:float
                                                  ]
                                                )
                         ] ;
              rdfs:comment "The ticket fee"@en ;
              rdfs:label "hasTicketFee"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasTitle
:hasTitle rdf:type owl:DatatypeProperty ;
          rdfs:domain :Review ;
          rdfs:range xsd:string ;
          rdfs:comment "The title of the review"@en ;
          rdfs:label "hasTitle"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasTripCode
:hasTripCode rdf:type owl:DatatypeProperty ;
             rdfs:domain :Trip ;
             rdfs:range xsd:string ;
             rdfs:comment "The trip code"@en ;
             rdfs:label "hasTripCode"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/hasWeight
:hasWeight rdf:type owl:DatatypeProperty ;
           rdfs:domain :Luggage ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:onDatatype xsd:float ;
                        owl:withRestrictions ( [ xsd:minExclusive "0.0"^^xsd:float
                                               ]
                                             )
                      ] ;
           rdfs:comment "The weight of the luggage"@en ;
           rdfs:label "hasWeight"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isAircraftModel
:isAircraftModel rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Aircraft ;
                 rdfs:range xsd:string ;
                 rdfs:comment "The aircraft model"@en ;
                 rdfs:label "isAircraftModel"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isRecommended
:isRecommended rdf:type owl:DatatypeProperty ;
               rdfs:domain :Review ;
               rdfs:range xsd:boolean ;
               rdfs:comment "If the flight is recommended"@en ;
               rdfs:label "isRecommended"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isSeatType
:isSeatType rdf:type owl:DatatypeProperty ;
            rdfs:domain :Seat ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:oneOf [ rdf:type rdf:List ;
                                     rdf:first "BusinessClass" ;
                                     rdf:rest [ rdf:type rdf:List ;
                                                rdf:first "EconomyClass" ;
                                                rdf:rest [ rdf:type rdf:List ;
                                                           rdf:first "FirstClass" ;
                                                           rdf:rest rdf:nil
                                                         ]
                                              ]
                                   ]
                       ] ;
            rdfs:comment "The seat type"@en ;
            rdfs:label "isSeatType"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isTypeOfTraveller
:isTypeOfTraveller rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Passenger ;
                   rdfs:range [ rdf:type rdfs:Datatype ;
                                owl:oneOf [ rdf:type rdf:List ;
                                            rdf:first "Business" ;
                                            rdf:rest [ rdf:type rdf:List ;
                                                       rdf:first "CoupleLeisure" ;
                                                       rdf:rest [ rdf:type rdf:List ;
                                                                  rdf:first "FamilyLesiure" ;
                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                             rdf:first "SoloLeisure" ;
                                                                             rdf:rest rdf:nil
                                                                           ]
                                                                ]
                                                     ]
                                          ]
                              ] ;
                   rdfs:comment "The type of the traveller"@en ;
                   rdfs:label "isTypeOfTraveller"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/isVerified
:isVerified rdf:type owl:DatatypeProperty ;
            rdfs:domain :Review ;
            rdfs:range xsd:boolean ;
            rdfs:comment "If the flight has been verified or not"@en ;
            rdfs:label "isVerified"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/knowsLanguages
:knowsLanguages rdf:type owl:DatatypeProperty ;
                rdfs:domain :Stewardess ;
                rdfs:range xsd:string ;
                rdfs:comment "The languages that a stewardess knows"@en ;
                rdfs:label "knowsLanguages"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/tripPurpose
:tripPurpose rdf:type owl:DatatypeProperty ;
             rdfs:domain :Trip ;
             rdfs:range xsd:string ;
             rdfs:comment "The purpose of the trip"@en ;
             rdfs:label "tripPurpose"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/Aircraft
:Aircraft rdf:type owl:Class ;
          rdfs:comment "Aircraft"@en ;
          rdfs:label "Aircraft"@en ;
          rdfs:sameAs "https://dbpedia.org/page/Aircraft"^^xsd:anyURI .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/AircraftManufacturer
:AircraftManufacturer rdf:type owl:Class ;
                      rdfs:comment "Aircraft Manufacturer"@en ;
                      rdfs:label "AircraftManufacturer"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/Airline
:Airline rdf:type owl:Class ;
         rdfs:comment "Airline"@en ;
         rdfs:label "Airline"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/Airport
:Airport rdf:type owl:Class ;
         rdfs:comment "Airport" ;
         rdfs:label "Airport"@en ;
         rdfs:sameAs "https://dbpedia.org/page/Airport"^^xsd:anyURI .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/ArrivalAt
:ArrivalAt rdf:type owl:Class ;
           rdfs:subClassOf :FlightDateTime ;
           rdfs:comment "Arrival time, date and timezone"@en ;
           rdfs:label "ArrivalAt"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/City
:City rdf:type owl:Class ;
      rdfs:comment "City"@en ;
      rdfs:label "City"@en ;
      rdfs:sameAs "https://dbpedia.org/page/City"^^xsd:anyURI .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/Country
:Country rdf:type owl:Class ;
         rdfs:comment "Country"@en ;
         rdfs:label "Country"@en ;
         rdfs:sameAs "https://dbpedia.org/page/Country"^^xsd:anyURI .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/CrewMember
:CrewMember rdf:type owl:Class ;
            rdfs:subClassOf :Person ;
            rdfs:comment "Crew members"@en ;
            rdfs:label "CrewMember"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/Delay
:Delay rdf:type owl:Class ;
       rdfs:comment "Delay"@en ;
       rdfs:label "Delay"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/FlightDateTime
:FlightDateTime rdf:type owl:Class ;
                rdfs:comment "The flights date and time"@en ;
                rdfs:label "FlightDateTime"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/Luggage
:Luggage rdf:type owl:Class ;
         rdfs:comment "Luggage"@en ;
         rdfs:label "Lugagge"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/Passenger
:Passenger rdf:type owl:Class ;
           rdfs:subClassOf :Person ;
           owl:disjointWith :Pilot ,
                            :Stewardess ;
           rdfs:comment "Passenger"@en ;
           rdfs:label "Passenger"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/Person
:Person rdf:type owl:Class ;
        rdfs:comment "Person" ;
        rdfs:label "Person"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/Pilot
:Pilot rdf:type owl:Class ;
       rdfs:subClassOf :CrewMember ;
       owl:disjointWith :Stewardess ;
       rdfs:comment "Pilot"@en ;
       rdfs:label "Pilot"@en .


###  http://www.semanticweb.org/documoda/ontologies/2024/3/esvOntology/Review
:Review rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasComment ;
                          owl:allValuesFrom xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasRating ;
